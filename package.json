{
  "name": "n8n-weather-workflow-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for n8n Daily Weather Email workflow",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:workflow": "jest tests/workflows/weather-email-workflow.test.js",
    "test:verbose": "jest --verbose",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --updateSnapshot",
    "clean": "rm -rf coverage node_modules",
    "lint": "eslint tests/**/*.js",
    "format": "prettier --write 'tests/**/*.js'"
  },
  "keywords": [
    "n8n",
    "workflow",
    "testing",
    "jest",
    "weather",
    "automation"
  ],
  "author": "Roy Kim - Customized Code",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
